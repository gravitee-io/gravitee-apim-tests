{
	"variables": [],
	"info": {
		"name": "Gravitee.io-Showcase-Policy.import-API-OAI",
		"_postman_id": "Gravitee.io-Showcase-Policy.import-API-OAI",
		"description": "",
		"schema": "https://schema.getpostman.com/json/collection/v2.0.0/collection.json"
	},
	"item": [
		{
			"name": "showcase.api.import-API-OAI",
			"description": "",
			"item": [
				{
					"name": "Import API from OAI",
					"event": [
						{
							"listen": "test",
							"script": {
								"type": "text/javascript",
								"exec": [
									"tests[\"Status code should be 201 / found \" + responseCode.code] = responseCode.code === 201;",
									"",
									"var jsonData = JSON.parse(responseBody);",
									"postman.setGlobalVariable(\"API_ID\", jsonData.id);"
								]
							}
						}
					],
					"request": {
						"url": "{{GRAVITEEIO_MGMT_URL}}/management/DEFAULT/apis/import/swagger",
						"method": "POST",
						"header": [
							{
								"key": "Content-Type",
								"value": "application/json",
								"description": ""
							},
							{
								"key": "Authorization",
								"value": "{{AUTHORIZATION_API1}}",
								"description": ""
							}
						],
						"body": {
							"mode": "raw",
							"raw": "{\"with_documentation\":true,\"with_path_mapping\":true,\"with_policy_paths\":true,\"with_policy_mocks\":true,\"type\":\"URL\",\"payload\":\"{{GRAVITEEIO_PORTAL_URL}}/portal/openapi\"}"
						},
						"description": ""
					},
					"response": []
				},
				{
					"name": "Get Page",
					"request": {
						"url": "{{GRAVITEEIO_MGMT_URL}}/management/DEFAULT/apis/{{API_ID}}/pages",
						"method": "GET",
						"header": [
							{
								"key": "Accept",
								"value": "application/json",
								"description": ""
							},
							{
								"key": "Authorization",
								"value": "{{AUTHORIZATION_API1}}",
								"description": ""
							}
						]
					},
					"event": [
						{
							"listen": "test",
							"script": {
								"type": "text/javascript",
								"exec": [
									"tests[\"Status code should be 200 / found \" + responseCode.code] = responseCode.code === 200;",
									"",
									"var jsonData = JSON.parse(responseBody);",
									"postman.setGlobalVariable(\"PAGE_ID\", jsonData[0].id);"
								]
							}
						}
					],
					"response": []
				},
				{
					"name": "Publish Page and enable try it",
					"request": {
						"url": "{{GRAVITEEIO_MGMT_URL}}/management/DEFAULT/apis/{{API_ID}}/pages/{{PAGE_ID}}",
						"method": "PATCH",
						"header": [
							{
								"key": "Content-Type",
								"value": "application/json",
								"description": ""
							},
							{
								"key": "Authorization",
								"value": "{{AUTHORIZATION_API1}}",
								"description": ""
							}
						],
						"body": {
							"mode": "raw",
							"raw": "{\"published\":true, \"configuration\": {\"tryIt\": \"true\", \"tryItAnonymous\": \"true\"}}"
						}
					}
				},
				{
					"name": "Create Keyless Plan",
					"event": [
						{
							"listen": "test",
							"script": {
								"type": "text/javascript",
								"exec": [
									"var plan = JSON.parse(responseBody);",
									"postman.setGlobalVariable(\"PLAN_ID\", plan.id);"
								]
							}
						}
					],
					"request": {
						"url": "{{GRAVITEEIO_MGMT_URL}}/management/DEFAULT/apis/{{API_ID}}/plans",
						"method": "POST",
						"header": [
							{
								"key": "Content-Type",
								"value": "application/json",
								"description": ""
							},
							{
								"key": "Authorization",
								"value": "{{AUTHORIZATION_API1}}",
								"description": ""
							}
						],
						"body": {
							"mode": "raw",
							"raw": "{\n\t\"name\": \"Freemium\",\n\t\"description\": \"Free Plan\",\n\t\"security\": \"key_less\",\n\t\"validation\": \"auto\",\n\t\"characteristics\": [],\n\t\"paths\": {\n\t\t\"/\": []\n\t}\n}"

						},
						"description": ""
					},
					"response": []
				},
				{
					"name": "Publish Plan",
					"request": {
						"url": "{{GRAVITEEIO_MGMT_URL}}/management/DEFAULT/apis/{{API_ID}}/plans/{{PLAN_ID}}/_publish",
						"method": "POST",
						"header": [
							{
								"key": "Accept",
								"value": "application/json",
								"description": ""
							},
							{
								"key": "Authorization",
								"value": "{{AUTHORIZATION_API1}}",
								"description": ""
							}
						]
					},
					"response": []
				},
				{
					"name": "Deploy API",
					"request": {
						"url": "{{GRAVITEEIO_MGMT_URL}}/management/DEFAULT/apis/{{API_ID}}/deploy",
						"method": "POST",
						"header": [
							{
								"key": "Content-Type",
								"value": "application/json",
								"description": ""
							},
							{
								"key": "Authorization",
								"value": "{{AUTHORIZATION_API1}}",
								"description": ""
							}
						],
						"body": {
							"mode": "raw",
							"raw": ""
						},
						"description": ""
					},
					"response": []
				},
				{
					"name": "Start API",
					"request": {
						"url": "{{GRAVITEEIO_MGMT_URL}}/management/DEFAULT/apis/{{API_ID}}?action=START",
						"method": "POST",
						"header": [
							{
								"key": "Authorization",
								"value": "{{AUTHORIZATION_API1}}",
								"description": ""
							}
						],
						"body": {
							"mode": "formdata",
							"formdata": []
						},
						"description": ""
					},
					"response": []
				},
				{
					"name": "Top API",
					"request": {
						"url": "{{GRAVITEEIO_MGMT_URL}}/management/DEFAULT/configuration/top-apis/",
						"method": "POST",
						"header": [
							{
								"key": "Content-Type",
								"value": "application/json"
							},
							{
								"key": "Authorization",
								"value": "{{AUTHORIZATION_ADMIN}}",
								"description": ""
							}
						],
						"body": {
							"mode": "raw",
							"raw": "{\"api\":\"{{API_ID}}\"}"
						},
						"description": ""
					},
					"response": []
				}
			]
		}
	]
}
